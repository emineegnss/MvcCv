
@{
    ViewBag.Title = "ExperienceAdd";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h4 style="color:crimson">Add New Experience Page</h4>
<br />

<form method="post">

    <input type="text" class="form-control" name="Title" placeholder="Enter Title" />
    <br />
    <input type="text" class="form-control" name="Subtitle" placeholder="Enter Subtitle" />
    <br />
    <textarea class="form-control" name="Description" placeholder="Enter Description" rows="5" ></textarea>
    <br />
    <input type="date" class="form-control" name="StartDate" placeholder="Enter Start Date" />
    <br />
    <input type="date" class="form-control" name="FinishDate" placeholder="Enter Finish Date" />
    <br />
    <button class="btn btn-info">Save</button>

</form>
